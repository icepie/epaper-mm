    "@nestjs/swagger": "^7.1.8",
    "@prisma/client": "5.2.0",
    "@types/jsdom": "^21.1.2",
    "canvas": "^2.11.2",
    "fabric": "^5.3.0",
    "jimp": "^0.22.10",
    "jsdom": "^22.1.0",
    "nestjs-pino": "^3.4.0",
    "nestjs-prisma": "^0.22.0",
